{
  "name": "bottr-cli",
  "version": "0.1.4",
  "description": "The command line tool for Bottr",
  "main": "index.js",
  "dependencies": {
    "commander": "^2.9.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.7.1"
  },
  "devDependencies": {},
  "scripts": {
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bottr-js/Bottr-CLI.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bottr-js/Bottr-CLI/issues"
  },
  "homepage": "https://github.com/Bottr-js/Bottr-CLI#readme",
  "bin": {
    "bottr-cli": "./index.js",
    "bottr": "./index.js"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "no-console": 0,
      "no-unused-vars": 0
    }
  }
}
